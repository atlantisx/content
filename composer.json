{
    "name": "atlantis/content",
    "description": "Atlantis X Content",
    "authors": [
        {
            "name": "Azri Jamil",
            "email": "azri@nematix.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "zendframework/zendpdf": "2.0.*@dev",
        "h4cc/wkhtmltopdf-i386": "*",
        "h4cc/wkhtmltopdf-amd64": "*",
        "h4cc/wkhtmltoimage-i386": "*",
        "h4cc/wkhtmltoimage-amd64": "*",
        "knplabs/knp-snappy": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers"
        ],
        "psr-0": {
            "Atlantis\\Content\\": "src/",
            "Atlantis\\Document\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
